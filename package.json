{
  "name": "aws-simple",
  "version": "4.1.0",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "keywords": [
    "apigateway",
    "aws",
    "cdk",
    "cloudformation",
    "deploy",
    "deployment",
    "lambda",
    "s3",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "types": "lib/types.d.ts",
  "bin": {
    "aws-simple": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc --build --pretty",
    "postbuild": "chmod +x lib/index.js",
    "ci": "run-p format:test update-readme lint test",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "prestart": "npm run build",
    "start": "node ./lib/index.js start",
    "test": "jest",
    "preupdate-readme": "npm run build",
    "update-readme": "node update-readme.js"
  },
  "dependencies": {
    "aws-sdk": "^2.800.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "cliui": "^7.0.4",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "ink": "^3.0.8",
    "ink-spinner": "^4.0.1",
    "lambda-local": "^1.7.4",
    "listr": "^0.14.3",
    "mime-types": "^2.1.27",
    "mkdirp": "^1.0.4",
    "p-retry": "^4.2.0",
    "prompts": "^2.4.0",
    "react": "^16.14.0",
    "signal-exit": "^3.0.3",
    "source-map-support": "^0.5.19",
    "url-join": "^4.0.1",
    "winston": "^3.3.3",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.75.0",
    "@aws-cdk/aws-certificatemanager": "^1.75.0",
    "@aws-cdk/aws-iam": "^1.75.0",
    "@aws-cdk/aws-lambda": "^1.75.0",
    "@aws-cdk/aws-route53": "^1.75.0",
    "@aws-cdk/aws-route53-targets": "^1.75.0",
    "@aws-cdk/aws-s3": "^1.75.0",
    "@aws-cdk/core": "^1.75.0",
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@types/aws-lambda": "^8.10.64",
    "@types/cli-spinners": "^1.3.0",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^1.0.1",
    "@types/prompts": "^2.0.9",
    "@types/react": "^17.0.0",
    "@types/signal-exit": "^3.0.0",
    "@types/url-join": "^4.0.0",
    "@types/yargs": "^15.0.10",
    "aws-cdk": "^1.75.0",
    "babel-jest": "^26.6.3",
    "ink-testing-library": "^2.1.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.75.0",
    "@aws-cdk/aws-certificatemanager": "^1.75.0",
    "@aws-cdk/aws-iam": "^1.75.0",
    "@aws-cdk/aws-lambda": "^1.75.0",
    "@aws-cdk/aws-route53": "^1.75.0",
    "@aws-cdk/aws-route53-targets": "^1.75.0",
    "@aws-cdk/aws-s3": "^1.75.0",
    "@aws-cdk/core": "^1.75.0",
    "aws-cdk": "^1.75.0"
  }
}
